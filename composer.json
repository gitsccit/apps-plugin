{
    "name": "gitsccit/apps",
    "description": "Apps plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "cakephp/cakephp": "^3.8",
        "gitsccit/web-skeleton": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "6.*"
    },
    "autoload": {
        "psr-4": {
            "Apps\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Apps\\Test\\": "tests/",
            "Apps\\Test\\Fixture\\": "tests/Fixture/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
